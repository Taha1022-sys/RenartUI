{"ast":null,"code":"import{useState,useEffect}from'react';import{productService}from'../services/api';export const useProducts=function(){let filters=arguments.length>0&&arguments[0]!==undefined?arguments[0]:{};const[products,setProducts]=useState([]);const[loading,setLoading]=useState(true);const[error,setError]=useState(null);const fetchProducts=async()=>{try{setLoading(true);setError(null);const data=Object.keys(filters).length>0?await productService.getProductsByFilters(filters):await productService.getAllProducts();setProducts(data);}catch(err){setError(err.message);}finally{setLoading(false);}};useEffect(()=>{fetchProducts();},[JSON.stringify(filters)]);return{products,loading,error,refetch:fetchProducts};};","map":null,"metadata":{},"sourceType":"module","externalDependencies":[]}